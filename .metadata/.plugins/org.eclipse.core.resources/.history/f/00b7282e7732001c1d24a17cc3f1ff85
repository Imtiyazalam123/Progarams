package com.imtiyaz;

import java.util.Scanner;

public class Palindrom2 {

	public static void main(String[] args) {

		int a = 100;
		int b = 1000;
		while(a <= b) {
			
			int reverse = 0;
			int org = a;
			while(a > 0) {
				
				int rem = a % 10;
				reverse = reverse * 10 + rem;
				a = a/10;
			}
			if(org == reverse) 
				System.out.print(org);
			a++;
		}
		
		

	}
}
